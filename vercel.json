{
  "headers": [
    {
      "source": "/assets/(.*)\\.mp4",
      "headers": [
        { "key": "Cache-Control", "value": "no-store" },
        { "key": "Accept-Ranges",  "value": "bytes" }
      ]
    },
    {
      "source": "/assets/(.*)\\.webm",
      "headers": [
        { "key": "Cache-Control", "value": "no-store" },
        { "key": "Accept-Ranges",  "value": "bytes" }
      ]
    }
  ]
}


